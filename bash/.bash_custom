#! /usr/bin/env bash

BINARY_PATHS=('')
for binary_path in ${BINARY_PATHS[@]}
do
    export PATH="$PATH:$binary_path"
done

alias clearhistory='history -c && history -w'
alias cdo='cd $OLDPWD'

agent () {
    [[ ! -z "$SSH_AGENT_PID" ]] && [[ -S "$SSH_AUTH_SOCK" ]] && echo "Agent pid $SSH_AGENT_PID"
    [[ ! -z "$SSH_AGENT_PID" ]] && [[ -S "$SSH_AUTH_SOCK" ]] || eval $(ssh-agent -t 30m)
}
alias noagent='eval $(ssh-agent -k)'
