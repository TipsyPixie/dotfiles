#! /usr/bin/env bash

alias psgrep='ps -ef | grep'
alias nsgrep='netstat -anop | grep'
alias lsgrep='ls | grep'

docker-shell() {
    [[ -z "$2" ]] && SHELL="bash"
    [[ -z "$2" ]] || SHELL="$2"
    
    docker exec -it $1 /bin/$SHELL
}

BINARY_PATHS=('')
for binary_path in ${BINARY_PATHS[@]}
do
    export PATH="$PATH:$binary_path"
done
